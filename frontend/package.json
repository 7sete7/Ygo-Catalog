  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/7sete7/Ygo-Catalog"
  },
  "dependencies": {
    "@coreui/coreui": "^2.0.2",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.2.0",
    "@coreui/icons": "^0.2.0",
    "@coreui/react": "^2.0.1",
    "bootstrap": "^4.1.1",
    "chart.js": "^2.7.2",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flag-icon-css": "^3.0.0",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-chartjs-2": "^2.7.2",
    "react-dom": "^16.4.0",
    "react-loadable": "^5.4.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.1",
    "react-test-renderer": "^16.4.0",
    "reactstrap": "^6.1.0",
    "simple-line-icons": "^2.4.1"
  },
  "devDependencies": {
    "babel-jest": "^23.0.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss --watch --recursive",
    "start-js": "react-scripts start --require dotenv/config index.js",
    "start": "npm-run-all -p watch-css -r dotenv/config dotenv_config_path=../.env start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
